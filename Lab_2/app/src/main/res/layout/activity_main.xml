<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:layout_width="183dp"
        android:layout_height="34dp"
        android:text="@string/moj_tekst"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.092" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="180dp"
        android:layout_height="34dp"
        android:text="@string/pierwszy_tekst"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="150dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="208dp"
        android:layout_height="41dp"
        android:text="@string/drugi_tekst"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="249dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="217dp"
        android:layout_height="42dp"
        android:text="@string/trzeci_tekst"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="353dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="213dp"
        android:layout_height="42dp"
        android:text="@string/czwarty_tekst"
        tools:layout_editor_absoluteX="116dp"
        tools:layout_editor_absoluteY="446dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="224dp"
        android:layout_height="40dp"
        android:text="@string/piaty_tekst"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="554dp" />

</androidx.constraintlayout.widget.ConstraintLayout>